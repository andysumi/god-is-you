{
    "extends": [
      "eslint:recommended"
    ],
    "plugins": [
      "googleappsscript"
    ],
    "parserOptions": {},
    "env": {
       "googleappsscript/googleappsscript": true
    },
    "globals": {
    },
    "rules": {
      "semi": "warn",
      "quotes": ["warn", "single"],
      "indent": ["warn", 2],
      "space-before-blocks": ["warn", { "functions": "always" }],
      "no-console": "off"
    }
}
